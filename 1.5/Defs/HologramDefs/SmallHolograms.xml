	<TabulaRasa.HologramDef>
		<defName>OuterRim_Hologram_Credits</defName>
		<label>credit sign</label>
		<hologramTags>
			<li>OuterRimHoloSmall</li>
		</hologramTags>
		<!-- First item in hologramLayers dictates the current glower colour for the projector. -->
