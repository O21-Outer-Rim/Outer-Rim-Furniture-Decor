	<!-- ==================== Dining Chair ==================== -->

	<ThingDef ParentName="OuterRimArtableFurnitureBase">
		<defName>OuterRim_TatooineDiningChair</defName>
		<label>dining chair (tatooine)</label>
		<description>A comfortable and attractive chair. Can be used at tables, work stations, and elsewhere.</description>
		<graphicData>
			<texPath>OuterRim/Building/Furniture/Tatooine/Tatooine_DiningChair</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<altitudeLayer>Building</altitudeLayer>
		<socialPropernessMatters>true</socialPropernessMatters>
		<pathCost>30</pathCost>
		<fillPercent>0.35</fillPercent>
		<defaultPlacingRot>South</defaultPlacingRot>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>8000</WorkToBuild>
			<Mass>5</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>8</Beauty>
			<Comfort>0.70</Comfort>
		</statBases>
		<building>
			<isSittable>true</isSittable>
		</building>
		<costList>
			<OuterRim_Durasteel>45</OuterRim_Durasteel>
		</costList>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<comps>
			<li Class="CompProperties_Styleable" />
		</comps>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>OuterRim_SubCatFurnitureTatooine</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

	<!-- ==================== Dresser ==================== -->

	<ThingDef ParentName="OuterRimArtableFurnitureBase">
		<defName>OuterRim_TatooineDresser</defName>
		<label>dresser (tatooine)</label>
		<description>A dresser. Gives a small comfort bonus to all nearby beds. Placing more than one dresser near the same bed has no effect.</description>
		<graphicData>
			<texPath>OuterRim/Building/Furniture/Tatooine/Tatooine_Dresser</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<size>(2,1)</size>
		<fillPercent>0.4</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<pathCost>42</pathCost>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<defaultPlacingRot>South</defaultPlacingRot>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Mass>15</Mass>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>5</Beauty>
		</statBases>
		<costList>
			<OuterRim_Durasteel>50</OuterRim_Durasteel>
		</costList>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<comps>
			<li Class="CompProperties_Facility">
				<statOffsets>
					<Comfort>0.05</Comfort>
				</statOffsets>
				<maxSimultaneous>1</maxSimultaneous>
				<maxDistance>6</maxDistance>
			</li>
		</comps>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>OuterRim_SubCatFurnitureTatooine</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

