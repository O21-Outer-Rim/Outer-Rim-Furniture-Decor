	<!-- ==================== 1x1 Table ==================== -->

	<ThingDef ParentName="OuterRimTableGatherSpotBase">
		<defName>OuterRim_Tatooine1x1Table</defName>
		<label>tatooine table (1x1)</label>
		<graphicData>
			<texPath>OuterRim/Building/Furniture/Tatooine/Tatooine_TableSmall</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1.05, 1.05)</drawSize>
		</graphicData>
		<size>(1, 1)</size>
		<defaultPlacingRot>South</defaultPlacingRot>
		<statBases>
			<Mass>15</Mass>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>800</WorkToBuild>
