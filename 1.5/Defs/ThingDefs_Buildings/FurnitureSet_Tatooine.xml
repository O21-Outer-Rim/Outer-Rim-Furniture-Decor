	<!-- ==================== Single Bed ==================== -->

	<ThingDef ParentName="OuterRimBedWithQualityBase">
		<defName>OuterRim_TatooineBed</defName>
		<label>bed (tatooine)</label>
		<description>A mattress and sheets on a frame for resting.</description>
		<graphicData>
			<texPath>OuterRim/Building/Furniture/Tatooine/Tatooine_Bed</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<size>(1,2)</size>
		<uiIconScale>0.90</uiIconScale>
		<fillPercent>0.4</fillPercent>
		<pathCost>42</pathCost>
		<staticSunShadowHeight>0.17</staticSunShadowHeight>
		<castEdgeShadows>True</castEdgeShadows>
		<statBases>
			<MaxHitPoints>140</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Beauty>1</Beauty>
			<WorkToBuild>800</WorkToBuild>
			<Mass>30</Mass>
			<BedRestEffectiveness>1</BedRestEffectiveness>
			<Comfort>0.75</Comfort>
			<ImmunityGainSpeedFactor>1.07</ImmunityGainSpeedFactor>
			<SurgerySuccessChanceFactor>1</SurgerySuccessChanceFactor>
		</statBases>
		<building>
			<bed_showSleeperBody>False</bed_showSleeperBody>
			<bed_healPerDay>4</bed_healPerDay>
		</building>
		<costList>
			<OuterRim_Durasteel>45</OuterRim_Durasteel>
		</costList>
		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li MayRequire="Ludeon.RimWorld.Ideology">SleepAccelerator</li>
					<!-- Outer Rim - Matching Set Bonus Only -->
					<li>OuterRim_TatooineEndTable</li>
					<li>OuterRim_TatooineDresser</li>
				</linkableFacilities>
			</li>
		</comps>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>OuterRim_SubCatFurnitureTatooine</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

